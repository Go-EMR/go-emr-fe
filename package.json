{
  "name": "goemr",
  "version": "1.0.0",
  "description": "GoEMR - Modern Healthcare Management Made Simple",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssl": "ng serve --ssl",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "cypress open",
    "e2e:headless": "cypress run",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss}\"",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/goemr/stats.json",
    "storybook": "ng run goemr:storybook",
    "build-storybook": "ng run goemr:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.2.18",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@ngrx/operators": "^19.0.0",
    "@ngrx/signals": "^19.0.0",
    "@primeng/themes": "^19.0.0",
    "chart.js": "^4.5.1",
    "date-fns": "^3.0.0",
    "lodash-es": "^4.17.21",
    "ng2-charts": "^6.0.0",
    "postcss": "^8.5.6",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular-eslint/builder": "^19.0.0",
    "@angular-eslint/eslint-plugin": "^19.0.0",
    "@angular-eslint/eslint-plugin-template": "^19.0.0",
    "@angular-eslint/schematics": "^19.0.0",
    "@angular-eslint/template-parser": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jest": "^29.5.0",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.23",
    "cypress": "^13.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.5.0",
    "webpack-bundle-analyzer": "^4.10.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
